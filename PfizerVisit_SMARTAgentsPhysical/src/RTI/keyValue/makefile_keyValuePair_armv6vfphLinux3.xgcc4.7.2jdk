JAVA_PATH = java
JAVAC_PATH = javac

JAVA_SOURCES = ./KeyValuePair.java ./KeyValuePairSeq.java ./KeyValuePairTypeSupport.java ./KeyValuePairTypeCode.java ./KeyValuePairDataReader.java ./KeyValuePairDataWriter.java ./KeyValuePairSubscriber.java ./KeyValuePairPublisher.java 

CLASS_FILES  = $(JAVA_SOURCES:%.java=%.class)

RTI_CLASSPATH := $(NDDSHOME)/class/nddsjava.jar
 
%.class : %.java
	$(JAVAC_PATH)  -classpath .:$(RTI_CLASSPATH) $<
 
all: $(CLASS_FILES)
 
#
# Convenient way to run the java programs
#

export LD_LIBRARY_PATH := $(NDDSHOME)/lib/armv6vfphLinux3.xgcc4.7.2jdk:/usr/lib/lwp:$(LD_LIBRARY_PATH)

KeyValuePairPublisher: ./KeyValuePairPublisher.class
	$(JAVA_PATH) -classpath ".:$(RTI_CLASSPATH)"  KeyValuePairPublisher $(ARGS)
 
KeyValuePairSubscriber: ./KeyValuePairSubscriber.class
	$(JAVA_PATH) -classpath ".:$(RTI_CLASSPATH)"  KeyValuePairSubscriber $(ARGS)

